{
  "name": "webpack-starter",
  "version": "1.0.0",
  "description": "Bare bones webpack project with a node express server",
  "main": "server/index.js",
  "scripts": {
    "dev": "webpack-dev-server --config ./webpack-dev.config.js --hot --progress --colors --content-base build & nodemon server/index.js --watch server",
    "build": "rm build/*; webpack --config ./webpack-prod.config.js --progress --colors",
    "start": "node server/index.js"
  },
  "repository": "git@github.com:cgreening/webpack-starter.git",
  "author": "Chris Greening",
  "license": "BSD",
  "dependencies": {
    "compression": "^1.4.4",
    "cors": "^2.6.0",
    "express": "^4.12.3"
  },
  "devDependencies": {
    "autoprefixer-loader": "^1.2.0",
    "babel-core": "^5.4.2",
    "babel-loader": "^5.1.0",
    "babel-runtime": "^5.4.2",
    "css-loader": "^0.12.1",
    "file-loader": "^0.8.1",
    "html-webpack-plugin": "^1.3.0",
    "image-webpack-loader": "^1.4.0",
    "node-libs-browser": "^0.5.0",
    "node-sass": "^3.1.1",
    "nodemon": "^1.3.7",
    "react": "^0.13.3",
    "react-favicon": "0.0.3",
    "react-hot-loader": "^1.2.7",
    "sass-loader": "^1.0.2",
    "style-loader": "^0.12.2",
    "webpack": "^1.9.6",
    "webpack-dev-server": "^1.8.2"
  }
}
